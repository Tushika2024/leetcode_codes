MY APPROACH-                                                                            TC=O(N)
bool lca(TreeNode *root,TreeNode* &ans,TreeNode *p,TreeNode *q){                        SC=O(HEIGHT)
        if(root==NULL){
            return false;
        }
        bool lans=lca(root->left,ans,p,q);
        bool rans=lca(root->right,ans,p,q);
        if(lans && rans){
            ans=root;
        }
        if(root==p || root==q){
            ans=root;
            return true;
        }
        return lans||rans;
    }
    TreeNode* lowestCommonAncestor(TreeNode* root, TreeNode* p, TreeNode* q) {
        TreeNode *ans;
         lca(root,ans,p,q);
        return ans;
    }

STRIVER-
TreeNode* lowestCommonAncestor(TreeNode* root, TreeNode* p, TreeNode* q) {
         if(root==NULL || root==p || root==q){
            return root;
        }
        TreeNode* lans=lowestCommonAncestor(root->left,p,q);
        TreeNode* rans=lowestCommonAncestor(root->right,p,q);
        if(lans==NULL){
            return rans;
        }
        else if(rans==NULL){
            return lans;
        }
        
            return root;
    }
