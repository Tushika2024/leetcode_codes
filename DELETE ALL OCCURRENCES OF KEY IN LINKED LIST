Node* delnode(Node* head,Node* temp){
        Node* prevnode=temp->prev;
        Node* nextnode=temp->next;
        if(temp==head){
            head=head->next;
            if(head!=NULL)head->prev=NULL;
            return head;
        }else if(temp->next==NULL){
            prevnode->next=NULL;
            temp->prev=NULL;
            delete temp;
            return head;
        }else{
            prevnode->next=temp->next;
            nextnode->prev=temp->prev;
            temp->next=NULL;
            temp->prev=NULL;
            return head;
        }
    }
    void deleteAllOccurOfX(struct Node** head_ref, int x) {
        // Write your code here
        Node* head=*head_ref;
        Node* temp=head;
        if(head==NULL || head->next==NULL) head=NULL;
        while(temp!=NULL){
            if(temp->data==x){
                Node* nextnode=temp->next;
                head=delnode(head,temp);
                temp=nextnode;
            }else temp=temp->next;
        }
        *head_ref=head;
    }
