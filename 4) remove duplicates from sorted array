BFA-
int removeDuplicates(vector<int>& nums) {
      set<int>st;
      for(int i=0;i<nums.size();i++){
            st.insert(nums[i]);
      }
      int idx=0;
      for(auto it:st){
        nums[idx]=it;
        idx++;
      }
      return idx;
    }

OPTIMAL-
 int removeDuplicates(vector<int>& nums) {
       int i=0;
       for(int j=1;j<nums.size();j++){
            if(nums[i]!=nums[j]){
                nums[++i]=nums[j];
            }
       }
       int k=i+1;
       return k;
    }
