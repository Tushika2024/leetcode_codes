
ListNode* findn(ListNode *temp,int n){
        int c=1;
        while(temp!=NULL){
            if(c==n) return temp;
            c++;
            temp=temp->next;
            
        }
        return temp;
    }
    ListNode* rotateRight(ListNode* head, int k) {
        if(head==NULL || head->next==NULL) return head;
        ListNode *temp=head;
        ListNode *tail=head;
        int size=0;
        while(tail->next!=NULL){
            size++;
            tail=tail->next;
        }
        tail->next=head;
        if(k%size==0) return head;
        k%=size;
        int diff=size-k;
        ListNode *nnode=findn(temp,diff);
        head=nnode->next;
        nnode->next=NULL;
        return head;
    }
