BFA-
 void rotate(vector<vector<int>>& matrix) {
        int n=matrix.size()-1;
        vector<vector<int>>ans(n + 1, vector<int>(n + 1));
        int k=0;
        int t=0;
        for(int i=n;i>=0;i--){
            for(int j=0;j<=n;j++){
                ans[k][t]=matrix[i][j];
                k++;
            }
            k=0;
            t++;
        }
        for(int i=0;i<=n;i++){
            for(int j=0;j<=n;j++){
                matrix[i][j]=ans[i][j];
            }
        }
    }
