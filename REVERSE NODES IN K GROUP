ListNode* reverse(ListNode* head){ TC=O(N)+O(N) SC=O(1)
        ListNode* prev=NULL;
        ListNode* curr=head;
        while(curr!=NULL){
            ListNode* nextpt=curr->next;
            curr->next=prev;
            prev=curr;
            curr=nextpt;
        }
        return prev;
    }
    ListNode* kthnode(ListNode* temp,int k){
        int c=1;
        while(temp!=NULL && c!=k){
            temp=temp->next;
            c++;
        }
        return temp;
    }
    ListNode* reverseKGroup(ListNode* head, int k) {
        if(head==NULL || head->next==NULL) return head;
        if(k==1) return head;
        ListNode *temp=head;
        ListNode *knode=NULL,*nextnode=NULL,*prevnode=NULL;
        while(temp!=NULL){
            knode=kthnode(temp,k);
            if(knode==NULL){
                if(prevnode!=NULL) prevnode->next=temp;
                break;
            }
            nextnode=knode->next;
            knode->next=NULL;
            reverse(temp);
            if(temp==head){
                head=knode;
                prevnode=temp;
            }else{
                prevnode->next=knode;
                prevnode=temp;
            }
            temp=nextnode;
        }
        return head;
    }
