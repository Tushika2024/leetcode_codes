BFA - USING STACK
OPTIMAL-
ListNode* reverseList(ListNode* head) {
        ListNode *prev=NULL;
        ListNode *curr=head;
        while(curr!=NULL){
            ListNode *next=curr->next;
            curr->next=prev;
            prev=curr;
            curr=next;
        }
        head=prev;
        return head;
    }
RECURSIVE
void reverse(ListNode* &head,ListNode *temp,ListNode *prev){   TC=O(N)  SC=O(N)
        if(temp->next==NULL){
            temp->next=prev;
            head=temp;
            return;
        }
        reverse(head,temp->next,temp);
        temp->next=prev;
        return;
    }
    ListNode* reverseList(ListNode* head) {
        if(head ==NULL || head->next ==NULL){
            return head;
        }
        ListNode *temp=head;
        reverse(head,temp,NULL);
        return head;
    }
