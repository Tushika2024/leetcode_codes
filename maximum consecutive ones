 int findMaxConsecutiveOnes(vector<int>& nums) {
        int c=0;
        int maxel=INT_MIN;
        for(int i=0;i<nums.size();i++){
            if(nums[i]==1){
                c++;
            }else{
                maxel=max(maxel,c);
                c=0;
            }
        }
        maxel=max(maxel,c);
        return maxel;
    }
