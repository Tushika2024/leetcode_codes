OPTIMAL-  RECURSIVE
int maxDepth(TreeNode* root) {
        if(root==NULL){
            return 0;
        }
        int ldepth=maxDepth(root->left);
        int rdepth=maxDepth(root->right);
        return max(ldepth,rdepth)+1;  
    }


LEVEL ORDER_
int maxDepth(TreeNode* root) {
        queue<TreeNode* >q;
        int c=0;
        q.push(root);
        while(!q.empty()){
            int size=q.size();
            for(int i=0;i<size;i++){
                TreeNode *el=q.front();
                if(el->left !=NULL){
                    q.push(el->left);
                }
                if(el->right !=NULL){
                    q.push(el->right);
                }
                q.pop();
            }
            c++;
        }
        return c;
    }
