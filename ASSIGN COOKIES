int findContentChildren(vector<int>& g, vector<int>& s) {                         TC=0(NLOGN + MLOGM + N/M)
        int gSize=g.size();                                                       SC=O(1)
        int cSize=s.size();
        sort(g.begin(),g.end());
        sort(s.begin(),s.end());
        int l=0;
        int r=0;
        while(l<gSize && r<cSize){
            if(s[r]>=g[l]){
                l++;
            }
            r++;
        }
        return l;
    }
