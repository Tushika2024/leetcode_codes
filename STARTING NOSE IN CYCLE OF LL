ListNode *detectCycle(ListNode *head) {
            ListNode *slow=head;
            ListNode *fast=head;
            int isCycle=0;
            while(fast !=NULL && fast->next !=NULL){
                slow=slow->next;
                fast=fast->next->next;
                if(slow==fast){
                    isCycle=1;
                    break;
                }
            }
            if(isCycle==1){
                slow=head;
                while(slow!=fast){
                    slow=slow->next;
                    fast=fast->next;
                }
                return slow;
            }
            return NULL;
    }
