 long long solve(vector<int>& bt) {                                TC=O(N LOGN)+O(N)
       long long wt=0;                                             SC=O(1)
       long long t=0;
       sort(bt.begin(),bt.end());
       for(int i=0;i<bt.size();i++){
           t+=wt;
           wt+=bt[i];
       }
       return t/bt.size();
        
    }
